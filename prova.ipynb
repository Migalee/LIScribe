    "import asyncio\n",
    "from bleak import BleakScanner\n",
    "\n",
    "async def scan():\n",
    "    devices = await BleakScanner.discover(timeout=5)\n",
    "    for d in devices:\n",
    "        print(f\"{d.name} – {d.address}\")\n",
    "\n",
    "await scan()"
